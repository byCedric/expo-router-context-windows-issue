{
  "configurations": [
    {
      "type": "node",
      "name": "Launch Program",
      "program": "${workspaceFolder}/node_modules/@expo/cli/build/bin/cli",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Attach to packager",
      "request": "attach",
      "type": "reactnative",
      "cwd": "${workspaceFolder}",
      "port": 19000,
      "skipFiles": [
        "node_modules/**",
      ]
    }
  ]
}
